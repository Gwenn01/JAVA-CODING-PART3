public class Main {
	public static void main(String[] args) {
		int[] arr = {1, 3, 4, 6, 7, 8, 9,};
		
		System.out.println(checkEvenOdd(arr));
		
	}
	//
	public static String checkEvenOdd(int[] arr){
	    String result = "";
	    String even = "Even", odd = "Odd";
	    
	    for(int i = 0; i < arr.length; i++){
	        if(arr[i]%2 == 0){
	            result += even + " ";
	        }else{
	            result += odd + " ";
	        }
	    }
	    return result;
	}
}