public class Main {
	public static void main(String[] args) {
		
		int[] num = {1, 1, 4, 1, 1, 1, 1};
		int uniqueNum = 0;
		for(int i = 0; i < num.length-1; i++){
		    for(int j = i+1; j < num.length; j++){
		        if(num[i] != num[j]){
		            uniqueNum = num[i];
		            break;
		        }
		    }
		}
		System.out.print(uniqueNum);
	}
}