public class Main {
	public static void main(String[] args) {
		int[] arr = {2, 1, 6, 4, 5, 3, 9, 7, 8};
		int result;
		for(int i = 0; i < arr.length; i++){
		    for(int j = i+1; j < arr.length; j++){
		        if(arr[i] > arr[j]){
		            result = arr[i];
		            arr[i] = arr[j];
		            arr[j] = result;
		        }
		    }
		}
		
		for(int i = 0; i < arr.length; i++){
		    System.out.print(arr[i]);
		}
	}
}