public class Main {
	public static void main(String[] args) {
		int[] arr = {3, 4, 3, 10};
		
		for(int i = 0; i < arr.length; i++){
		    for(int j = i+1; j < arr.length; j++){
		        if(arr[i] == arr[j]){
		            arr[i] = 0;
		            arr[j] = 0;
		        }
		    }
		}
		
		int sum = 0;
		for(int i = 0; i < arr.length; i++){
		    sum += arr[i];
		}
	System.out.print(sum);	
	}
}