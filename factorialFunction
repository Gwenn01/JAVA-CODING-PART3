import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		
		System.out.print("Enter a number: ");
		int num = s.nextInt();
		
		System.out.print(factorial(num));
		
	}
	public static int factorial(int num){
	    int temp = num;
	    
	    while(num != 1){
	        num--;
	        temp = temp * num;
	    }
	    return temp;
	}
}