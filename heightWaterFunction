public class Main {
	public static void main(String[] args) {
		int[] height = {1,8,6,2,5,4,8,3,7};
		
		System.out.println(water(height));
	}
	public static int water(int[] height){
	    int max = height[0];
	    
	    for(int i = 0; i < height.length; i++){
	        if(height[i] > max){
	            max = height[i];
	        }
	    }
	    
	    int result = max * max;
	    
	    
	    return result;
	}
}